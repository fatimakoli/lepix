fun main () : int {
	var a : int = 24 * 2 + 1;
	// a == 49
	var b : int = a % 8;
	// b == 1
	b = a;
	// b == 49

	var c : int[3] = [10, 20, 30];
	a = c[2] + 10;
	// a == 40

	var d : int[[3,3]] = [[1, 2, 3], [4, 5, 6], [7, 8, 9]];
	b = d[0][0] + d[1][1];
	// b == 6
	c[2] = b;
	// c[2] == 6
	return 0;
}
